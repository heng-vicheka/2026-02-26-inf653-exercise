<div>
    <table>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Date</th>
            <th>Status</th>
            <th>Action</th>
        </tr>

        {{#each data}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.name}}</td>
            <td>{{this.date}}</td>
            <td>{{this.status}}</td>
            <td>
                <a href="/items/{{this.id}}">View Details</a>
            </td>
        </tr>
        {{/each}}
    </table>
</div>